#################################################################
#    Data properties
#################################################################

###  https://w3id.org/iot-tta#FireFly_nodes
:FireFly_nodes rdf:type owl:DatatypeProperty ;
               rdfs:domain :Wireless_sensor_network ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :FireFly_nodes ;
                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                  owl:onDatatype xsd:integer ;
                                                  owl:withRestrictions ( [ xsd:minInclusive 20
                                                                         ]
                                                                         [ xsd:maxInclusive 32
                                                                         ]
                                                                       )
                                                ]
                           ] ;
               rdfs:range xsd:integer ;
               rdfs:comment "fireFly_nodes (Data property): in a FireFly wireless sensor network, each gateway is set to manage between 20 and 32 network nodes"^^xsd:string ;
               rdfs:label "fireFly_nodes"^^xsd:string .


###  https://w3id.org/iot-tta#MicroSD_card
:MicroSD_card rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Raspberyy_Pi_4B ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :MicroSD_card ;
                            owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                 owl:onDatatype xsd:double ;
                                                 owl:withRestrictions ( [ xsd:minInclusive "8.0"^^xsd:double
                                                                        ]
                                                                      )
                                               ]
                          ] ;
              rdfs:range xsd:double ;
              rdfs:comment "microSD_card (Data property): setting up Raspberry Pi 4 Model B requires a microSD card with a capacity of at least 8GB"^^xsd:string ;
              rdfs:label "microSD_card"^^xsd:string .


###  https://w3id.org/iot-tta#Password_mobile_node
:Password_mobile_node rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Wireless_sensor_network ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :Password_mobile_node ;
                                    owl:hasValue "password"
                                  ] ;
                      rdfs:range xsd:string ;
                      rdfs:comment "password_mobile_node (Data property): in a FireFly wireless sensor network, a mobile node logins with a password of at most 100 bytes"^^xsd:string ;
                      rdfs:label "password_mobile_node"^^xsd:string .


###  https://w3id.org/iot-tta#Power_supply
:Power_supply rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Raspberyy_Pi_4B ,
